void helper(vector<int>&ans,vector<int> &arr,int N,int index,int sum){
    if(index==N){
        ans.push_back(sum);
        return;
    }
    //pick the element
    helper(ans,arr,N,index+1,sum+arr[index]);
    //not pickup
    helper(ans,arr,N,index+1,sum);
}
 vector<int> subsetSums(vector<int> arr, int N)
    {
        vector<int>ans;
        helper(ans,arr,N,0,0);
        return ans;
  }
